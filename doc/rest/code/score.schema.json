{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "score",
  "type": "object",
  "properties": {
    "id": {
      "type": "number"
    },
    "title": {
      "type": "string"
    },
    "price": {
      "exclusiveMinimum": 0,
      "type": "number"
    },
    "difficulty": {
      "minimum": 1,
      "maximum": 5,
      "type": "number"
    },
    "category": {
      "type": "string"
    }
  },
  "required": ["id", "title", "price", "difficulty"]
}