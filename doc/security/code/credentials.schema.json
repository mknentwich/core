{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "credentials",
  "type": "object",
  "properties": {
    "email": {
      "type": "string",
      "format": "email"
    },
    "password": {
      "type": "string",
      "minLength": 12,
      "maxLength": 128
    }
  },
  "required": [
    "email",
    "password"
  ]
}